   "source": [
    "df_train = pd.read_csv(PATH_INPUT + \"/train.csv\")\n",
    "df_greeks = pd.read_csv(PATH_INPUT + \"/greeks.csv\")\n",
    "df_train_greeks = pd.merge(df_train, df_greeks, on=\"Id\")\n",
    "s = setup(\n",
    "    data=df_train_greeks,  # データセット\n",
    "    target='Alpha',  # 目的変数名\n",
    "    log_experiment=True,  # 実験記録を保存\n",
    "    experiment_name='ICR_proto',  # 実験記録の名前\n",
    "    categorical_features=[],  # カテゴリカル変数列を指定\n",
    "    numeric_features=[],  # 数値変数列を指定\n",
    "    ignore_features=[\"Id\", \"Class\", \"Beta\", \"Gamma\", \"Delta\", \"Epsilon\"],  # 除外列を指定\n",
    "    # remove_outliers=True,  # 外れ値処理の有無\n",
    "    # polynomial_features=True,  # 多項式特徴量の作成有無\n",
    "    # polynomial_degree=2, # 多項式の次数\n",
    "    # feature_selection=True,  # 変数選択の有無\n",
    "    session_id=0,  # 再現性確保のため、シード値を固定\n",
    ")"
   ]
